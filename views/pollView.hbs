<div class="container">
  <h1>{{this.name}}</h1>
	<table>
	  {{#each this.options}}
	  	<tr>
	  		<td><button type="button" class="btn btn-success btn-sm"><span class="glyphicon glyphicon-thumbs-up" aria-hidden="true"></span></button></td>
	  		<td><button type="button" class="btn btn-danger btn-sm"><span class="glyphicon glyphicon-thumbs-down" aria-hidden="true"></span></button></td>
	  		<td><h3>{{this.options.name}}</h3></td>
	  	</tr>
	  {{/each}}
	</table>
</div>
<script src="https://cdn.socket.io/socket.io-1.4.5.js"></script>
<script type="text/javascript" src="/javascript/socketclient.js"></script>
